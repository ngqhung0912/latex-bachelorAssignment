	issn = {0032-5910},
	journal = {Powder Technology},
	keywords = {Angle of repose, Geotechnical, Granular, Measurements},
	pages = {397-417},
	title = {A review on the angle of repose of granular materials},
	volume = {330},
	year = {2018},
	bdsk-url-1 = {https://www.sciencedirect.com/science/article/pii/S0032591018301153},
