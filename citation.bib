	doi = {10.1016/j.cpc.2019.107129},
	issn = {0010-4655},
	journal = {Computer Physics Communications},
	keywords = {Granular materials, DEM, DPM, MercuryDPM, Open-source},
	pages = {107129},
	title = {{Fast, flexible particle simulations~-~An introduction to MercuryDPM}},
	volume = {249},
	year = {2020},
